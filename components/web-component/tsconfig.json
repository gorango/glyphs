{
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "@data/*": ["data/*"]
    },
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "module": "esnext",
    "strict": false,
    "target": "es2017",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": [
    "src",
    "types/jsx.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
