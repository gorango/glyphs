    "gen:wc:core": "cd wc/core && glyphs generate",
    "gen:wc:flags": "cd wc/flags && glyphs generate",
    "sv:react:brands": "cd react/brands && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/react-brands",
    "sv:react:core": "cd react/core && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/react-core",
    "sv:react:flags": "cd react/flags && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/react-flags",
    "sv:vue:brands": "cd vue/brands && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/vue-brands",
    "sv:vue:core": "cd vue/core && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/vue-core",
    "sv:vue:flags": "cd vue/flags && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/vue-flags",
    "sv:wc:brands": "cd wc/brands && standard-version --path . --skip.commit --skip.tag --skip.changelog -t @glyphs/wc-brands",
