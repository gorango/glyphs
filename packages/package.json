    "pub:react:flags": "cd react/flags && npm publish --access public",
    "pub:vue:brands": "cd vue/brands && npm publish --access public",
    "pub:vue:core": "cd vue/core && npm publish --access public",
    "pub:vue:flags": "cd vue/flags && npm publish --access public",
    "pub:wc:brands": "cd wc/brands && npm publish --access public",
    "pub:wc:core": "cd wc/core && npm publish --access public",
    "pub:wc:flags": "cd wc/flags && npm publish --access public",
    "ncu": "(npm run ncu:react:brands);(npm run ncu:react:core);(npm run ncu:react:flags);(npm run ncu:vue:brands);(npm run ncu:vue:core);(npm run ncu:vue:flags);(npm run ncu:wc:brands);(npm run ncu:wc:core);(npm run ncu:wc:flags)",
    "gen": "(npm run gen:react:brands);(npm run gen:react:core);(npm run gen:react:flags);(npm run gen:vue:brands);(npm run gen:vue:core);(npm run gen:vue:flags);(npm run gen:wc:brands);(npm run gen:wc:core);(npm run gen:wc:flags)",
    "sv": "(npm run sv:react:brands);(npm run sv:react:core);(npm run sv:react:flags);(npm run sv:vue:brands);(npm run sv:vue:core);(npm run sv:vue:flags);(npm run sv:wc:brands);(npm run sv:wc:core);(npm run sv:wc:flags)",
    "pub": "(npm run pub:react:brands);(npm run pub:react:core);(npm run pub:react:flags);(npm run pub:vue:brands);(npm run pub:vue:core);(npm run pub:vue:flags);(npm run pub:wc:brands);(npm run pub:wc:core);(npm run pub:wc:flags)"
  }
