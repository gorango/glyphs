{
  "scripts": {
    "ncu:react:brands": "cd react-brands && npx npm-check-updates -u && yarn",
    "ncu:react:core": "cd react-core && npx npm-check-updates -u && yarn",
    "ncu:react:flags": "cd react-flags && npx npm-check-updates -u && yarn",
    "ncu:vue:brands": "cd vue-brands && npx npm-check-updates -u && yarn",
    "ncu:vue:core": "cd vue-core && npx npm-check-updates -u && yarn",
    "ncu:vue:flags": "cd vue-flags && npx npm-check-updates -u && yarn",
    "ncu:wc:brands": "cd wc-brands && npx npm-check-updates -u && yarn",
    "ncu:wc:core": "cd wc-core && npx npm-check-updates -u && yarn",
    "ncu:wc:flags": "cd wc-flags && npx npm-check-updates -u && yarn",
    "gen:react:brands": "cd react-brands && glyphs generate",
    "gen:react:core": "cd react-core && glyphs generate",
    "gen:react:flags": "cd react-flags && glyphs generate",
    "gen:vue:brands": "cd vue-brands && glyphs generate",
    "gen:vue:core": "cd vue-core && glyphs generate",
    "gen:vue:flags": "cd vue-flags && glyphs generate",
    "gen:wc:brands": "cd wc-brands && glyphs generate",
    "gen:wc:core": "cd wc-core && glyphs generate",
    "gen:wc:flags": "cd wc-flags && glyphs generate",
    "build:react:brands": "cd react-brands && npm run build",
    "build:react:core": "cd react-core && npm run build",
    "build:react:flags": "cd react-flags && npm run build",
    "build:vue:brands": "cd vue-brands && npm run build",
    "build:vue:core": "cd vue-core && npm run build",
    "build:vue:flags": "cd vue-flags && npm run build",
    "build:wc:brands": "cd wc-brands && npm run build",
    "build:wc:core": "cd wc-core && npm run build",
    "build:wc:flags": "cd wc-flags && npm run build",
    "sv:react:brands": "cd react-brands && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:react:core": "cd react-core && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:react:flags": "cd react-flags && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:vue:brands": "cd vue-brands && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:vue:core": "cd vue-core && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:vue:flags": "cd vue-flags && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:wc:brands": "cd wc-brands && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:wc:core": "cd wc-core && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "sv:wc:flags": "cd wc-flags && standard-version --path . --skip.commit --skip.tag --skip.changelog",
    "pub:react:brands": "cd react-brands && npm publish --access public",
    "pub:react:core": "cd react-core && npm publish --access public",
    "pub:react:flags": "cd react-flags && npm publish --access public",
    "pub:vue:brands": "cd vue-brands && npm publish --access public",
    "pub:vue:core": "cd vue-core && npm publish --access public",
    "pub:vue:flags": "cd vue-flags && npm publish --access public",
    "pub:wc:brands": "cd wc-brands && npm publish --access public",
    "pub:wc:core": "cd wc-core && npm publish --access public",
    "pub:wc:flags": "cd wc-flags && npm publish --access public",
    "ncu:react": "run-s ncu:react:*",
    "ncu:vue": "run-s ncu:vue:*",
    "ncu:wc": "run-s ncu:wc:*",
    "gen:react": "run-s gen:react:*",
    "gen:vue": "run-s gen:vue:*",
    "gen:wc": "run-s gen:wc:*",
    "build:react": "run-s build:react:*",
    "build:vue": "run-s build:vue:*",
    "build:wc": "run-s build:wc:*",
    "sv:react": "run-s sv:react:*",
    "sv:vue": "run-s sv:vue:*",
    "sv:wc": "run-s sv:wc:*",
    "pub:react": "run-s pub:react:*",
    "pub:vue": "run-s pub:vue:*",
    "pub:wc": "run-s pub:wc:*",
    "ncu": "(npm run ncu:react);(npm run ncu:vue);(npm run ncu:wc)",
    "gen": "(npm run gen:react);(npm run gen:vue);(npm run gen:wc)",
    "build": "(npm run build:react);(npm run build:vue);(npm run build:wc)",
    "sv": "(npm run sv:react);(npm run sv:vue);(npm run sv:wc)",
    "pub": "(npm run pub:react);(npm run pub:vue);(npm run pub:wc)",
    "all": "(npm run ncu);(npm run gen);(npm run build);(npm run sv)",
    "svg:sync:brands": "cd brands && glyphs sync -d . -s . -p --set brands --diff",
    "svg:sync:core": "cd core && glyphs sync -d . -s . -p --set core",
    "svg:sync:flags": "cd flags && glyphs sync -d . -s . -p --set flags --diff",
    "svg:sv:brands": "cd brands && standard-version -i changelog.md --path . -t @glyphs/brands-v",
    "svg:sv:core": "cd core && standard-version -i changelog.md --path . -t @glyphs/core-v",
    "svg:sv:flags": "cd flags && standard-version -i changelog.md --path . -t @glyphs/flags-v",
    "svg:pub:brands": "cd brands && npm publish --access public",
    "svg:pub:core": "cd core && npm publish --access public",
    "svg:pub:flags": "cd flags && npm publish --access public",
    "svg:sync": "(npm run svg:sync:brands);(npm run svg:sync:core);(npm run svg:sync:flags)",
    "svg:sv": "(npm run svg:sv:brands);(npm run svg:sv:core);(npm run svg:sv:flags)",
    "svg:pub": "(npm run svg:pub:brands);(npm run svg:pub:core);(npm run svg:pub:flags)",
    "svg": "(npm run svg:sync);(npm run svg:sv)"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}